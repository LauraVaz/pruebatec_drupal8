{{ attach_library('mydata/adplus-lib') }}

<div class="container">
<h2>Crousel Publicidades</h2>
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!--Indicators -->
        <ol class="carousel-indicators">
                    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
                    <li data-target="#myCarousel" data-slide-to="1"></li>
                    <li data-target="#myCarousel" data-slide-to="2"></li>

        </ol>
        <!--Wrapper for slides -->
        <div class="carousel-inner">
            {% if value|length != 0 %}
            <div class="item-active">
                <img class="card-img"  src="{{ value[0].url }}" alt="">
            </div>
            {% endif %}
            {% for item in value %}
            <div class="item">
                <img class="card-img"  src="{{ item.url }}" alt="">
            </div>
          {% endfor %}
        </div>
        <!-- Left and rigth controls -->
        <a class="left carousel-control" href="#myCarousel"  data-sslide="prev">
        <span class="glyphicon glyphicon-chevron-left "> </span>
        <span class="sr-only">Previous</span></a>
        <a class="right carousel-control" href="#myCarousel"  data-sslide="next">
            <span class="glyphicon glyphicon-chevron-right "> </span>
            <span class="sr-only">Next</span></a>
        </div>
    </div>


